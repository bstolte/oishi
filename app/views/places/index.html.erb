<% @places.each do |place| %>
	<div class="booyah-box col-xs-10 col-xs-offset-1">
		<h1><%= place.name %></h1><br />
		<i><%= place.address %></i><br /><br />
		<p><%= place.description %></p>
	</div>
<% end %>

<div class="container">
	<div class="col-xs-4 col-xs-offset-4">
		<%= paginate @places %>
	</div>
</div>
